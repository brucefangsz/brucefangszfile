{
    "compilerOptions": {
        "outDir": "./out/",
        "sourceMap": true,
        "noImplicitAny": true,
        "module": "commonjs",
        "target": "es5",
        "jsx": "react",
        "strictNullChecks": true,
        "lib": [
            "es5",
            "dom",
            "es2015.promise",
            "es2015.iterable",
            "es2015.symbol.wellknown"
        ]
    },
    "exclude": [
        "node_modules",
        "out"
    ]
}